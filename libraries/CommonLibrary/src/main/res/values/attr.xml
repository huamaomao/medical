<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="FilterImageView">
        <attr name="filterColor"  format="color" />
    </declare-styleable>

    <declare-styleable name="BaseEmptyView">
        <attr name="fadeAnimation" format="boolean"/>
    </declare-styleable>
    <declare-styleable name="BasicEmptyView">
        <attr name="textPadding" format="dimension"/>
        <attr name="textSizeSP" format="integer"/>
        <attr name="emptyText" format="string"/>
        <attr name="errorText" format="string"/>
    </declare-styleable>
    <declare-styleable name="CustomEmptyView">
        <attr name="emptyLayout" format="reference"/>
        <attr name="errorLayout" format="reference"/>
        <attr name="loadingLayout" format="reference"/>
    </declare-styleable>
</resources>